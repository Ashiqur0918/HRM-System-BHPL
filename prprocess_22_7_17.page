page prprocess


    caption "Payroll Process - Trial Run"
    section form one, method getPSamt, method getESamt, ~
    	method getTRamt, method getADVamt, ~
    	method getITamt, method chk_access

    form one
      layout 1
      object  Process,Show,~
              xyear default(#cyear),xper default(#cper),~
			  dbase,xsec caption(Section),xempf,xempt

      field xptype
        attrib local
        display radio
        caption Select Processing Type
        pick "Test Run,Final Run"
        default "Test Run"
      end field
	  
		field xdiv
			pick "select xcode from xcodes where zid='"+#id+"' and xtype='unit'"
		end field

		field xsec
			display hide
			pick "select xcode from xcodes where zid='"+#id+"' and xtype='Section'"
		end field
		

      field dbase
      	display const
        attrib local
        storage varchar
        caption Basis        
        pick "Annual,Month"
        def "Month"
      end field

      field pfund
        attrib local
        storage varchar
        caption <font color=red>Provident Fund
        display combo
        pick "Yes,No"
        def "Yes"
      end field

      field xempf
         pick list xempnum
         width 10
		 default "0"
         event before
            //set globals(xfemp)=xfemp
         end event
      end field

      field xempt
         //pick list xtemp
		 pick list xempnum
         width 10
         default "z"
         event before
            //set globals(xtemp)=xtemp
         end event
      end field

      field Process
        event before
          set globals(ErrChk) = ""
          set globals(dbase_days) = 0.0D

          int prvper = 0+xper-1
		  call lastday

          str startdate = ""
          if xper<10
            str payyrpr = ""+xyear+"0"+xper	
          else
            str payyrpr = ""+xyear+""+xper
          end if

          int offday = 0
          set payday = #days(xdatepay)-#days(prvdate)+1
		  //error payday+" "+xdatepay+" "+prvdate

          if xper .eq. "01"
          	set mth = "January"
          else if xper .eq. "02"
          	set mth = "February"
          else if xper .eq. "03"
          	set mth = "March"
          else if xper .eq. "04"
          	set mth = "April"
          else if xper .eq. "05"
          	set mth = "May"
          else if xper .eq. "06"
          	set mth = "June"
          else if xper .eq. "07"
          	set mth = "July"
          else if xper .eq. "08"
          	set mth = "August"
          else if xper .eq. "09"
          	set mth = "September"
          else if xper .eq. "10"
          	set mth = "October"
          else if xper .eq. "11"
          	set mth = "November"
          else if xper .eq. "12"
          	set mth = "December"
          end if

          if globals(ErrChk) .ne. "1"
			set flagpr=#sql(str,"select xpflag from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"'")
	
			if #result .eq. "true" .and. flagpr .eq. "1"
				set globals(ErrChk) = "1"
				error "<font color=red size=++>Final Process Completed Already</font>"
			end if
			
          end if

          if globals(ErrChk) .ne. "1"
			set dbase="Month"
	          if dbase .eq. "Month"
                    set chekday=0+chkday
                    if xper .eq. "01" .or. xper .eq. "1"
                        set prev_per=0+12
                        set prev_yr=0+xyear-1
                    else
                        set prev_per=0+xper-1
                        set prev_yr=0+xyear
                    end if
                    set startdate = prev_yr+"-"+prev_per+"-"+chekday			
    	            set globals(dbase_days) = #days(xdatepay)-#days(startdate)    	            
	          else if dbase .eq. "Annual"
	            set startdate = payyr+"-01-01"
	            str tmpdate = payyr+"-12-31"
	            set globals(dbase_days) = #days(tmpdate)-#days(startdate)
	          end if
	      end if

			if globals(ErrChk) .ne. "1"
          		set basic_code = #sql("select xprbasic from prdef where zid='"+#id+"' and xprbasicapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+basic_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>Basic Code ("+basic_code +") not found in Payment Head Table</font>"
					end if
          		end if
			end if

			if globals(ErrChk) .ne. "1"
          		set gross_code = #sql("select xprgross from prdef where zid='"+#id+"' and xprgrossapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+gross_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>Gross Salary Code ("+gross_code +") not found in Payment Head Table</font>"
					end if
          		end if
			end if

	        if globals(ErrChk) .ne. "1"
          		set pf_code = #sql("select xprpf from prdef where zid='"+#id+"' and xprpfapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+pf_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>P.F. Code ("+pf_code +") not found in Payment Head Table</font>"
					else
						set xpfempcont = prdef.xpfempcont("zid='"+#id+"'")
						set xpfcomcont = prdef.xpfcomcont("zid='"+#id+"'")
						if xpfempcont <= 0.0
							set globals(ErrChk) = "1"
							error "<font color=red size=+1>P.F. Employee Contribution% not found</font>"
						end if
						if xpfcomcont < 0.0
							set globals(ErrChk) = "1"
							error "<font color=red size=+1>P.F. Company Contribution% not found</font>"
						end if
					end if
          		end if
          	end if

	        if globals(ErrChk) .ne. "1"
          		set bonus_code = #sql("select xprbonus from prdef where zid='"+#id+"' and xprbonusapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+bonus_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>Bonus Code ("+bonus_code +") not found in Payment Head Table</font>"
					end if
          		end if
          	end if

	        if globals(ErrChk) .ne. "1"
          		set tax_code = #sql("select xpritax from prdef where zid='"+#id+"' and xpritaxapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+tax_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>Income Tax Code ("+tax_code +") not found in Payment Head Table</font>"
					end if
          		end if
          	end if

	        if globals(ErrChk) .ne. "1"
          		set arrear_code = #sql("select xprarrear from prdef where zid='"+#id+"' and xprarrearapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+arrear_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>Arrear Salary Code ("+arrear_code +") not found in Payment Head Table</font>"
					end if
          		end if
          	end if

	        if globals(ErrChk) .ne. "1"
          		set hrent_code = #sql("select xprhrent from prdef where zid='"+#id+"' and xprhrentapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+hrent_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>House Rent Code ("+hrent_code +") not found in Payment Head Table</font>"
					end if
          		end if
          	end if

	        if globals(ErrChk) .ne. "1"
          		set suspend_code = #sql("select xprsusp from prdef where zid='"+#id+"' and xprsuspapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+suspend_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>Suspension Code ("+suspend_code +") not found in Payment Head Table</font>"
					end if
          		end if
          	end if

	        if globals(ErrChk) .ne. "1"
          		set subsist_code = #sql("select xprsubsist from prdef where zid='"+#id+"' and xprsubsistapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+subsist_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>Subsistence Allowance Code ("+subsist_code +") not found in Payment Head Table</font>"
					end if
          		end if
          	end if

	        if globals(ErrChk) .ne. "1"
          		set absent_code = #sql("select xprabsent from prdef where zid='"+#id+"' and xprabsentapp='Yes'")
          		if #result .eq. "true"
					set chkcode = #sql(str,"select xpaycode from prpayhead where zid='"+#id+"' and xpaycode='"+absent_code+"'")
					if #result .eq. "false"
						set globals(ErrChk) = "1"
						error "<font color=red size=+1>Absent Deduction Code ("+absent_code +") not found in Payment Head Table</font>"
					end if
          		end if
          	end if

          if globals(ErrChk) .ne. "1"
            	set chkrec = #sql("select xdatepay from prpay where zid='"+#id+"' and xdatepay='"+xdatepay+"'")
            	if #result .ne. "true"
					set myqry = "insert into prpay (ztime,zid,xdatepay,xremarks,zemail,xemail,xyear,xper)"
					set myqry = myqry + " values('"+#time+"','"+#id+"','"+xdatepay+"','','"+#user+"','','"+xyear+"',~
					'"+xper+"')"
					set tmpstr = #sql(myqry)
            	end if
          end if

          if globals(ErrChk) .ne. "1"
            set globals(xempnum) = xempf
			set mysql = "select premp.xempnum from premp inner join hrmst on hrmst.zid=premp.zid and hrmst.xempnum=premp.xempnum where premp.zid='"+#id+"' ~
									and premp.zactive='1' and premp.xempnum>='"+globals(xempnum)+"' and ~
									premp.xempnum<='"+xempt+"' order by premp.zid,premp.xempnum"
            set globals(xempnum) = #sql(str,mysql)
			
            while #result .eq. "true" .and. globals(xempnum) .ne. ""
			//print globals(xempnum)
			  set globals(probstat) = "No"
              set globals(resigstat) = "No"
              set globals(resigyrpr) = ""
              set globals(poststat) = ""
              set globals(HNPstat) = ""
              set globals(abday) = 0
              int dayab = 0
              set globals(lvday) = 0.0
              //set globals(xpaycode) = ""
              set globals(xpsclcode) = ""
              set globals(payamt) = 0.0D
              set tmpflag = ""
              set xpflag = ""

              if globals(ErrChk) .ne. "1"
				set tmpstr = #sql(str,"select xmodepay from premp where zid='"+#id+"' and xempnum='"+globals(xempnum)+"'")
                //set globals(xpsclcode) = #sql(str,"select xpsclcode from hrmst where zid='"+#id+"' and xempnum='"+globals(xempnum)+"'")
				set globals(xpsclcode) = #sql(str,"select xpsclcode from premp where zid='"+#id+"' and xempnum='"+globals(xempnum)+"'")
				set xjoindate = #sql("select xjoindate from premp where zid='"+#id+"' and xempnum='"+globals(xempnum)+"'")
				set tmpyrpr = ""+#sub(xjoindate,0,4)+""+#sub(xjoindate,5,2)
		////error "xjoindate  :"+xjoindate+" tmpyrpr  :"+tmpyrpr+" payyrpr  :"+payyrpr
				if payyrpr .eq. tmpyrpr
                    set chekday=0+chkday+1
                    if xper .eq. "01" .or. xper .eq. "1"
                        set prev_per=0+12
                        set prev_yr=0+xyear-1
                    else
						
                        set prev_per=0+xper
                        set prev_yr=0+xyear
						
                    end if
					
					set prev_perlen=#len(prev_per)
					set chekdaylen=#len(chekday)
					
					if prev_perlen>1
						set prev_per=prev_per
					else
						set prev_per="0"+prev_per
					end if
					
					if chekdaylen>1
						set chekday=chekday
					else
						set chekday="0"+chekday
					end if
					
					set tmpdate = ""+prev_yr+"-"+prev_per+"-"+chekday
	
					set tmpstr = #sub(xjoindate,0,10)
					//error tmpstr+" "+tmpdate
					set offday = 0

					set offday = #days(tmpstr)-#days(tmpdate)
					//error offday+ " "+"123456"+" "+tmpstr+" "+tmpdate
					set offday = offday + globals(abday)
					//error offday+ " "+"10"
					set globals(abday) = offday
					
					///error globals(abday)
					///error "join:"+globals(abday)+"  - "+xjoindate+" - "+tmpdate
					set globals(probstat) = "No"
					set globals(resigstat) = "No"
					set globals(resigyrpr) = ""
				end if
				set zactive=#sql("select zactive from premp where zid='"+#id+"' and xempnum='"+xempnum+"'")
          		set chkrec = #sql("select xdatepay from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")        
				if #result .eq. "true"
					set dayab = #sql(int,"select xdayab from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")
					set daysuspend = #sql(int,"select xdaysuspend from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")
					set daysuspenq = #sql(int,"select xdaysuspenq from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")
					set suspendf = #sql(decimal,"select xsuspendf from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")
					set suspenqf = #sql(decimal,"select xsuspenqf from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")
					
					set globals(abday) = 0+globals(abday)+dayab
				
					set globals(lvday) = #sql(decimal,"select xratev from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")
					set xadvflag = #sql("select xadvflag from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")
					set xpflag = #sql("select xpflag from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'")
          		else
					set myqry = "insert into prmsg (ztime,zid,xdatepay,xempnum,xremarks,xdayab,xdaylv,xnhour,~
          				xgvothour,xnoothour,xresth,xpflag,xadvflag,zemail,xemail,xratev,xday,xdate,~
						xdaysuspend,xdaysuspenq,xsuspendf,xsuspenqf,xref,zactive)"
					set myqry = myqry + " values('"+#time+"','"+#id+"','"+xdatepay+"','"+xempnum+"','',"0","0","0",~
						"0","0","0",'','Yes','"+#user+"','',"+globals(lvday)+","0",'"+#date+"',"0","0","0","+globals(abday)+",'','"+zactive+"')"
					set tmpstr = #sql(myqry)
					set xadvflag = "Yes"
          		end if

                if globals(poststat) .ne. "1"
                  set tmpflag = ""
                  if globals(resigstat) .eq. "Yes" .and. payyrpr .gt. globals(resigyrpr)
                    set tmpflag = "1"
                  else if globals(HNPstat) .eq. "1"
                    set tmpflag = "1"
                  else
                  	set tmpflag = xpflag
                  end if

                  if tmpflag .ne. "1"
                  	//print "checked"
					set mysql = "delete from practrndt where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'"
					set tmpstr = #sql(mysql)

                    set globals(xpaycode) = ""
                    double totpay = 0.0D
                    double totded = 0.0D
                    double basic = 0.0D
                    double gross = 0.0D
                    double totsuspend = 0.0D
                    double totsubsist = 0.0D

                    set mysql = "select xpaycode from prpayhead where zid='"+#id+"' and xpaycode>'"+globals(xpaycode)+"' order by xpaycode"
                    set globals(xpaycode) = #sql(str,mysql)
		//error #result+"globals(xpaycode) "+globals(xpaycode) 
                    while #result .eq. "true" .and. globals(xpaycode) .ne. ""
                      	double actamt = 0.0D
                      	set globals(payamt) = 0.0D
                      	set xprsign = #sql("select xprsign from prpayhead where zid='"+#id+"' and xpaycode='"+globals(xpaycode)+"'")
                      	set xdesc = #sql("select xdesc from prpayhead where zid='"+#id+"' and xpaycode='"+globals(xpaycode)+"'")
                      	set xdigit = #sql(int,"select xdigit from prpayhead where zid='"+#id+"' and xpaycode='"+globals(xpaycode)+"'")
						set xoption = #sql(str,"select xoption from prpayhead where zid='"+#id+"' and xpaycode='"+globals(xpaycode)+"'")
	//print globals(xpaycode)
                      	if xprsign .sw. "3"
                        	set globals(payamt) = 0.0D+totpay
							if globals(xpaycode) .eq. gross_code
								set gross = 0.0D+globals(payamt)
							end if

                      	else if xprsign .sw. "4"
                        	set globals(payamt) = 0.0D+totded

                      	else if xprsign .sw. "5"
                        	set globals(payamt) = 0.0D+totpay-totded

                      	else
                      		//if globals(xpaycode) .eq. tax_code
                      			//call getITamt
                      		//else
                      			//print globals(xpaycode)
	                    		call getPSamt
                        		//print "after ps amount="+globals(payamt)
                        		call getESamt
                        		//print "after es amount="+globals(payamt)
                        		call getTRamt
                        		//print "after tr amount="+globals(payamt)
								//print globals(xpaycode)+" "+xprsign+" "+xadvflag
								if globals(xpaycode) .eq. basic_code
									set basic = 0.0D+globals(payamt)
								end if

								//if totsuspend > 0.0
								//	print totsuspend //+" "+actamt+" "+globals(xpaycode)
								//end if
							//end if
							//print globals(xpaycode)+" "+xprsign
                        	if xprsign .sw. "2"
								//print "test tamal "+globals(xpaycode)+" xprsign "+xprsign+" "+xadvflag
                          		if xadvflag .eq. "Yes"
                            		call getADVamt
                          		end if
                        	end if

                        	if xdigit > 0
                          		set globals(payamt) = 0.0D+#round(globals(payamt),xdigit)
                        	else if xdigit < 0
                          		set globals(payamt) = 0.0D+#round(globals(payamt),0)
                        	end if

                        if xprsign .sw. "1"
                          set totpay = 0.0D+totpay+globals(payamt)
                        else if xprsign .sw. "2"
		                    if globals(xpaycode) .eq. suspend_code
		                    	call suspend_calc
		                    	set globals(payamt) = 0.0D+totsuspend
								//print "B: "+basic+" G: "+gross

							end if
		                    if globals(xpaycode) .eq. subsist_code
		                    	call subsist_calc
		                    	set globals(payamt) = 0.0D+totsubsist
		                    	//print globals(xpayamt)+" "+totsubsist+" "+subsist_code
							end if

		                    if globals(xpaycode) .eq. pf_code
		                    	set xpfflag = premp.xpfflag("zid='"+#id+"' and xempnum='"+globals(xempnum)+"'")
//								print xpfflag +" ok"
		                    	if xpfflag .eq. "Yes"
		                    		compute totpf = basic*(xpfempcont/100)
		                    		set globals(payamt) = 0.0D+totpf
		                    		//print globals(xpayamt)+" "+totpf
		                    	end if
							end if
								if globals(payamt) < 0.0D
									set globals(payamt)=0.0D-globals(payamt)
								end if 
							//print globals(payamt)+" "+globals(xpaycode)+" "+xempnum
	                    	set totded = 0.0D+totded+globals(payamt)
                        end if
                      end if

                      //print "found;"+globals(xpaycode)+";"+globals(abday)+";"+globals(payamt)

                      if globals(payamt) > 0.0D
                         //print globals(xpaycode)+" "+globals(payamt)
                      	if xpflag .ne. "1"
                      		//print globals(xpaycode)

                      		set xoption = prpayhead.xoption("zid='"+#id+"' and xpaycode='"+globals(xpaycode)+"'")
                      		//print xoption
                      		if xoption .ne. "Advance"
                      			int xrow = 0+10
								set myqry = "insert into practrndt (ztime,zutime,zid,xdatepay,xempnum,xpaycode,xrow,xamount,zemail,xemail)"
								set myqry = myqry + " values('"+#time+"','"+#time+"','"+#id+"','"+xdatepay+"','"+globals(xempnum)+"','"+globals(xpaycode)+"',~
									'"+xrow+"','"+globals(payamt)+"','"+#user+"','')"
								set tmpstr =#sql(myqry)
								//print #result
							end if
						end if
                      end if

                      set mysql = "select xpaycode from prpayhead where zid='"+#id+"' and xpaycode>'"+globals(xpaycode)+"' order by xpaycode"
                      set globals(xpaycode) = #sql(str,mysql)
                    end while
                  end if
                    //error globals(resigstat)+" - "+globals(resigyrpr)
                  if globals(resigstat) .eq. "Yes" .and. payyrpr .gt. globals(resigyrpr)
                    set mysql = "delete from prtrndt where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'"
                    set mysql = #sql(mysql)

                    set mysql = "delete from prmsg where zid='"+#id+"' and xdatepay='"+xdatepay+"' and xempnum='"+globals(xempnum)+"'"
                    set mysql = #sql(mysql)
                  else
                  end if
                end if
              end if

